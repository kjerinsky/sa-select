{
  "name": "SA-Select",
  "version": "0.0.1",
  "description": "Select autocomplete",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "sselect.js"
      ],
      "css": [
        "sselect.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "manifest_version": 2
}